<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.rental.fragment.RentalFragment">

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabRental"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:tabIndicatorColor="@color/primary"
        app:tabMode="scrollable"
        app:tabTextColor="@color/primary" />

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPagerRental"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp">


    </androidx.viewpager.widget.ViewPager>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="650dp"
        android:layout_marginTop="1100dp"
        android:layout_marginBottom="6dp"
        android:backgroundTint="#ff3030"
        android:src="@drawable/ic_round_add_24"
        app:layout_constraintBottom_toTopOf="@+id/viewPagerRental"
        app:layout_constraintEnd_toStartOf="@+id/tabRental"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></com.google.android.material.floatingactionbutton.FloatingActionButton>
</androidx.constraintlayout.widget.ConstraintLayout>